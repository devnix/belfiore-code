{
    "name": "devnix/belfiore-code",
    "description": "Serialized data of Italian Belfiore codes and foreign region codes",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Devnix\\BelfioreCode\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devnix\\BelfioreCode\\": "src-dev/",
            "Devnix\\BelfioreCode\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "ext-zip": "*",
        "symfony/console": "^4.4|^5.0",
        "symfony/serializer": "^4.4|^5.0",
        "symfony/error-handler": "^4.4|^5.0",
        "symfony/yaml": "^4.4|^5.0",
        "devnix/zip-exception": "^0.1.0",
        "phpoffice/phpspreadsheet": "^1.10",
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.88",
        "rector/rector": "^2.2"
    },
    "require": {
        "php": "^8.1",
        "doctrine/collections": "^2.3"
    },
    "authors": [
        {
            "name": "Pablo Largo Mohedano",
            "email": "devnix.code@gmail.com"
        }
    ]
}
